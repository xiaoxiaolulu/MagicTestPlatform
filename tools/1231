  `refuse_rate` int(3) DEFAULT '0',
  `auto_refuse_count` int(11) DEFAULT '0',
  `preordered_count` int(11) DEFAULT '0',
  `mem_wechat_group_date` date DEFAULT NULL COMMENT '加入微信群组日期',
  `car_steward_phone` varchar(20) DEFAULT NULL COMMENT '车管家手机',
  `car_delegation_date` date DEFAULT NULL COMMENT '托管日期',
  `is_first_passed` int(1) DEFAULT '1' COMMENT '是否第一次通过审核 1:第一次, 2:不是第一次,3:初始数据为已发',
