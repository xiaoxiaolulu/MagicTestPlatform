  `insurance_reason` text COMMENT '车辆交强险审核不通过理由',
  `car_delegation_start_time` date DEFAULT NULL COMMENT '车辆托管开始时间',
  `car_delegation_end_time` date DEFAULT NULL COMMENT '车辆托管结束时间',
