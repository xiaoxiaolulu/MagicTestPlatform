  `exigence_con_phone` varchar(20) DEFAULT '' COMMENT '紧急联系人电话',
  `last_modify_date` timestamp NOT NULL DEFAULT '0000-00-00 00:00:00' COMMENT '车主修改车辆信息，最后一次修改时间',
  `seq_score` double(5,2) DEFAULT '0.00' COMMENT '车辆排序分',
  `spare_key` int(1) DEFAULT '0' COMMENT '是否有备用钥匙:0否1是',
  `update_time` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `car_steward_name` varchar(200) DEFAULT NULL COMMENT '车管家姓名',
