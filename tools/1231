  `vehicle_vice_lic` varchar(100) DEFAULT NULL COMMENT '车辆行驶证副页照片路径',
  `vehicle_vice_status` int(1) NOT NULL DEFAULT '0' COMMENT '行驶证审核状态：0 待审核 1已通过 2未通过',
  `vehicle_vice_advise` varchar(500) DEFAULT NULL COMMENT '反面不符合要求，填写拍照建议',
  `vehicle_lic_advise` varchar(500) DEFAULT NULL COMMENT '正面面不符合要求，填写拍照建议',
  `audio_interface` int(1) NOT NULL DEFAULT '2' COMMENT '外接音源接口, 1:有 2：无',
  `child_safety_seat` int(1) NOT NULL DEFAULT '2' COMMENT '儿童安全座椅, 1:有 2：无',
  `competitive_score` int(11) NOT NULL DEFAULT '0' COMMENT '车辆竞争力分数',
  `ly_car_param_source` int(11) DEFAULT NULL COMMENT '0-国产，1-合资，2-进口',
  `recommend_level` int(1) DEFAULT '2' COMMENT '推荐级别:1、强烈推荐 2、一般 3、不推荐',
  `engine_source` int(11) DEFAULT '2' COMMENT '动力源：1-纯电动，2-汽油，3-油电混动，4-柴油，5-天然气，6-石油气',
  `weekend_price` int(11) DEFAULT NULL COMMENT '周末价',
  `weekend_special_price` int(11) DEFAULT NULL COMMENT '周末特供价',
  `specific_pic` tinyint(4) DEFAULT '0' COMMENT '是否显示个性照片 0不显示 1显示',
  `new_drive_type` int(1) DEFAULT '0' COMMENT '新驱动方式，3: 前轮驱动，4: 后轮驱动，5: 全时四驱，6: 分时四驱，7: 适时四驱，8四轮驱动',
  `luggage_num` int(2) DEFAULT '0' COMMENT '可放置24寸行李箱个数',
  `min_luggage_volume` int(5) DEFAULT '0' COMMENT '行李厢最小容量,单位L',
  `max_luggage_volume` int(5) DEFAULT '0' COMMENT '行李厢最大容量,单位L',
  `proxy_expire` date DEFAULT NULL COMMENT '车主授权委托书到期日期',
  `proxy_reason` varchar(500) DEFAULT NULL COMMENT '车辆委托授权书审核不通过理由',
  `is_accept_set_price` tinyint(4) DEFAULT '0' COMMENT '是否接受平台智能定价:1-接受,0-不接受',
  `car_level` tinyint(3) unsigned DEFAULT NULL COMMENT '车辆等级,1: A,2: B,3:C,4: D,5:E,6:F,7: F+,8: SUV A, 9:SUV B,10:SUV C,11:SUV D,12:SUV E,13: SUV F,14:SUV F+,15:MPV A,16:MPV B,17:MPV C,18:MPV D,19:MPV E,20:MPV F,21:MPV F+,22:跑车',
  `garage_id` int(11) DEFAULT NULL COMMENT '车行id',
  PRIMARY KEY (`id`),
  UNIQUE KEY `reg_no_idx` (`reg_no`) USING BTREE,
  KEY `mem_id_idx` (`mem_no`) USING BTREE,
  KEY `search_car_idx` (`brand`,`type`,`year`,`model`) USING BTREE,
  KEY `car_city_idx` (`city`) USING BTREE,
  KEY `status_city_idx` (`status`,`city`) USING BTREE,
  KEY `frame_no_idx` (`frame_no`) USING BTREE,
  KEY `type_idx` (`type`),
  KEY `reg_time_idx` (`reg_time`)
) ENGINE=InnoDB AUTO_INCREMENT=286742 DEFAULT CHARSET=utf8;

CREATE DEFINER=`auto_dev`@`%` TRIGGER `car_individuality_url_update_trigger` BEFORE UPDATE ON `atzuchedb`.`car` FOR EACH ROW BEGIN
                        IF new.individuality_url = '' THEN
			       SET new.individuality_url = NULL;
			END IF;
    END;